<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="TextCustom">
        <item name="android:layout_marginTop">@dimen/element_divide_margin</item>
        <item name="android:textAlignment">center</item>
        <item name="android:textSize">@dimen/textNormal</item>
    </style>
    <style name="buttonCustom">
        <item name="android:layout_width">@dimen/btn_width</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/btnText</item>
    </style>


</resources>
